{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES6",
        "removeComments": true,
        "preserveConstEnums": true,

        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "strictNullChecks": true,
    }
}
